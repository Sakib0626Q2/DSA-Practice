    "def linScarch(num, target):\n",
    "    for i in range(len(num)):\n",
    "        if num[i] == target:\n",
    "            return i\n",
    "    return -1\n",
    "\n",
    "num = [4, 2, 3, 5, 7, 9, 11]\n",
    "target = 7\n",
    "result = linScarch(num, target)\n",
    "print(\"Target found at index:\", result)  # Output: Target found at index: 4"
   ]
